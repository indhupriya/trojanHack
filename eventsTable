CREATE TABLE `Events` 
(
	`EVENT_ID`	INTEGER NOT NULL primary key AUTO_INCREMENT,
	`EVENT_NAME`	varchar2,
	`EVENT_CATEGORY` varchar(20),
	`EVENT_DATE`	varchar(20),
	`EVENT_URL`	varchar(20),
	`EVENT_DESC`	varchar(20),
	`EVENT_TIME`	varchar(20),
	`EVENT_UPVOTE`	INTEGER DEFAULT 1,
	`EVENT_DOWNVOTE`	INTEGER DEFAULT 0
);
